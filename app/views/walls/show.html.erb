<div id="info">
  <br/>
  <p>
    <%= image_tag @wall.logo.url, class: "logo" if @wall.has_logo? %>
  </p>
  <div class="description <%= @wall.font_color %> <%= @wall.font_style %>">
    <%= parse_hypecode @wall.description %>
  </div>
</div>

<div id="wall" class="wall" data-frames-url="<%= frame_wall_path(@wall,@wall_instance_id) %>"></div>